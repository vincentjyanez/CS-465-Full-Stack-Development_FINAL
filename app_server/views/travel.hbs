<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Dive Sites - Bhaccasyoniztas Beach Resort Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header }}	
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Travel</h1>
							<ul id="sites">
			                    {{#each trips}}					
								<li>
									<a href="/travel/">
									
										<img src="{{this.image}}" alt="Img">
									</a>
									<a href="/api/trips/{{this.code}}"><img src="images/</a>"
									<h2>
										<a href="/travel/">
										{{this.title}}</a>
									</h2>
									<p>
										{{this.description}}
									</p>
									<p>
										{{this.additionalInfo}}
									</p>
									<!-- Edit Link -->
                                    <a href="/travel/edit/{{this_id}}">Edit Trip</a>
								</li>
			                    {{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer }}
			<p>
				Â© 2023 by BHACCASYONIZTAS BEACH RESORT. All Rights Reserved
			</p>
		</div>
</body>
</html>